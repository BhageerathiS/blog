<style>
.container {
 background-image: url(images/ico.png)
  }
</style>

<!DOCTYPE html>
<html>
  <head>
    <title>| Test Blog |</title>
    <%= csrf_meta_tags %>
    <%= favicon_link_tag 'ico.png' %>
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
    <meta name="theme-color" content="#08698c">
  </head>

  <body class="container">
    <a href="#bottom" class="back-bottom text-center" onclick="$('html, body').animate({scrollTop: $(document).height()}, 1000); return false;">
      <i class="fa fa-arrow-down"></i>
    </a>
    
      
        <!-- GA Tracking Code for Buyers -->
        <script>
          (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
          (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
          m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
          })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');
          <% if Rails.env.production? %>
            ga('create', 'UA-71252343-1', 'auto');
          <% else %>
            ga('create', 'UA-81659992-1', 'auto');
          <% end %>
          ga('send', 'pageview');
        </script>
      
      <div class="wrapper">
        
        <!-- Page Content -->
        <div class="page-content-wrapper">
          <div class="container main">
            <%= yield %>
          </div>  
          <%#= render "layouts/footer" %>
        </div>
        <!-- /#page-content-wrapper -->
      </div>  
    

   
    <div class="st-dis-click"></div>
    <a href="#top" class="back-top text-center" onclick="$('body, html').animate({scrollTop: 0}, 500); return false;">
      <i class="fa fa-arrow-up"></i>
    </a>
  </body>
</html>

